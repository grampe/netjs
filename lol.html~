<html>
<script type="text/javascript" src="raphael.js"></script>
<script type="text/javascript" src="jquery.js"></script>

<body>
<script>
var r=10;
var paper = Raphael(0, 0, 600, 800);

$(document).ready(function() {    
	$.getJSON(
		"net.json",
		function(data){
			r = data.radius;
			cir= paper.circle(r, r, r;)			
		}
	);
});
document.write(r);

var a = Array();
for(i=0;i<10;i++)
{
	a[i] = paper.circle(r + i*r*2, r+ i*r*2, r);
	a[i].attr("fill", "#f00");
	a[i].attr("stroke", "#fff");	
}
</script>
